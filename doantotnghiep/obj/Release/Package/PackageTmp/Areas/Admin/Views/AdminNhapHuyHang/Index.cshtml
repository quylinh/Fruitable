@* ====================== Index.cshtml ====================== *@
@model IEnumerable<doantotnghiep.Models.HuyNhapHang>

@{ ViewBag.Title = "Danh sách phiếu hủy hàng"; }

<h2>Danh sách phiếu hủy hàng</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Mã sản phẩm</th>
            <th>Lý do hủy</th>
            <th>Ngày hủy</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
<tr>
    <td>@item.MaSP</td>
    <td>@item.LyDoHuy</td>
    <td>@item.CreateDate.ToString("dd/MM/yyyy HH:mm")</td>
</tr>}
    </tbody>
</table>